<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
  <ImageView android:id="@+id/icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/yylogo"/>
  <LinearLayout android:id="@+id/floats" android:layout_width="270dp" android:layout_height="60dp" android:orientation="horizontal" android:background="@drawable/blackfloat" android:visibility="gone">
    <ImageButton android:id="@+id/raidersbag" android:layout_height="40dp" android:layout_gravity="center_vertical" android:layout_marginLeft="20dp" android:layout_width="40dp" android:background="@drawable/raiders_bag"/>
    <ImageButton android:id="@+id/moregame" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="10dp" android:layout_gravity="center_vertical" android:background="@drawable/moregame"/>
    <ImageButton android:id="@+id/onlinecustom" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="10dp" android:layout_gravity="center_vertical" android:background="@drawable/onlinecustom"/>
    <ImageButton android:id="@+id/acountmanager" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="10dp" android:layout_gravity="center_vertical" android:background="@drawable/account_manager"/>
    <ImageButton android:id="@+id/gamebox" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="10dp" android:layout_gravity="center_vertical" android:background="@drawable/gamebox"/>
  </LinearLayout>
</LinearLayout>